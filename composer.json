{
    "name" : "makinacorpus/goat",
    "type": "library",
    "description": "Yet another database connector",
    "keywords": ["orm", "postgresql", "mysql", "database", "symfony", "pdo"],
    "homepage": "http://github.com/pounard/goat",
    "license": "GPL-2",
    "authors": [
        {
            "name" : "Pierre RINEAU",
            "email" : "pierre.rineau@processus.org"
        }
    ],
    "autoload" : {
        "exclude-from-classmap": ["/Tests/"],
        "psr-0": {
            "WeakRef" : "compat/"
        },
        "psr-4" : {
            "Goat\\" : "."
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Goat\\Benchmark\\": "benchmarks"
        }
    },
    "require" : {
        "ocramius/generated-hydrator": "^2.0",
        "php" : ">=7.0",
        "symfony/event-dispatcher": "^3.2"
    },
    "require-dev" : {
        "phpunit/phpunit" : "5.*",
        "phpbench/phpbench": "^0.13.0",
        "symfony/http-foundation" : "^3.2",
        "squizlabs/php_codesniffer": "^2.7"
    },
    "suggest" : {
        "ext-weakref" : "for a better transaction handling",
        "ocramius/generated-hydrator": "^2.0",
        "symfony/event-dispatcher" : "^3.2"
    }
}
